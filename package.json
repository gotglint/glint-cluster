{
  "name": "@gotglint/glint-cluster",
  "version": "0.0.2",
  "description": "Glint Cluster",
  "author": "Michael Laccetti <michael@laccetti.com> (https://laccetti.com)",
  "homepage": "https://github.com/gotglint/glint-cluster/",
  "repository": {
    "type": "git",
    "url": "https://github.com/gotglint/glint-cluster.git"
  },
  "bugs": {
    "url": "https://github.com/gotglint/glint-cluster/issues"
  },
  "license": "GPL-3.0",
  "scripts": {
    "build": "$(which gulp)",
    "watch": "$(which gulp) watch",
    "debug": "node debug $(which gulp) test:no-coverage",
    "debug-remote": "node --debug-brk=5858 $(which gulp) test:no-coverage",
    "docs": "$(which gulp) docs"
  },
  "files": [
    "run",
    "LICENSE",
    "README.md",
    "src"
  ],
  "bin": {
    "glint": "run"
  },
  "dependencies": {
    "@gotglint/glint-client": "0.0.5",
    "binary-pack": "1.0.2",
    "bluebird": "3.4.3",
    "bufferutil": "1.2.1",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "intel": "1.1.1",
    "jsonfn": "0.31.0",
    "lodash": "4.15.0",
    "node-uuid": "1.4.7",
    "object-sizeof": "^1.1.0",
    "primus": "6.0.2",
    "statsd-client": "0.2.2",
    "utf-8-validate": "1.2.1",
    "uws": "0.8.0",
    "ws": "1.1.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "child_process": "1.0.2",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-eslint": "3.0.1",
    "gulp-istanbul": "1.1.1",
    "gulp-mocha": "3.0.1",
    "gulp-nodemon": "2.1.0",
    "mocha": "3.0.2",
    "mr-doc-theme-glint": "0.0.2",
    "run-sequence": "1.2.2"
  }
}
